<h1>Classroom</h1>
<table border="1">
    <tr>
        <td>Id</td>
        <td>Title</td>
        <td>Description</td>
        <td>Enabled</td>
        <td colspan="2" style="text-align: center">Action</td>
    </tr>
    {% for i in tabclass %}
        <tr>
            <td>{{ i.id }}</td>
            <td>{{ i.title }}</td>
            <td>{{ i.description }}</td>
            <td>{{ i.enable }}</td>
            <td><a href="{{ path('updateClassroom',{'id':i.id}) }}">update</a></td>
            <td><a href="{{ path('removeClassroom',{'id':i.id}) }}">delete</a></td>

        </tr>
    {% endfor %}
</table>